   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", 
     RowBox[{"(", 
      RowBox[{"A", "+", 
       RowBox[{
        SqrtBox["2"], " ", 
        SqrtBox["En"], " ", 
        SqrtBox["m"]}]}], ")"}], " ", "x"}]], " ", 
   RowBox[{"C", "[", "1", "]"}]}], "+", 
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "\[ImaginaryI]"}], " ", 
     RowBox[{"(", 
      RowBox[{"A", "-", 
       RowBox[{
        SqrtBox["2"], " ", 
        SqrtBox["En"], " ", 
        SqrtBox["m"]}]}], ")"}], " ", "x"}]], " ", 
   RowBox[{"C", "[", "2", "]"}]}]}]], "Output",
 CellChangeTimes->{3.739701959918296*^9, 3.739763933292409*^9, 
