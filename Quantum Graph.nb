Cell[BoxData[
 RowBox[{"eqn", "=", 
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"-", "1"}], 
      RowBox[{"2", " ", "m", " "}]], 
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"\[CapitalPsi]", "[", "x", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "2"}], "}"}]}], "]"}]}], "-", 
    RowBox[{
     FractionBox[
      RowBox[{"A", " ", "I"}], "m"], 
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"\[CapitalPsi]", "[", "x", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "1"}], "}"}]}], "]"}]}], "+", 
    RowBox[{
     FractionBox[
      SuperscriptBox["A", "2"], 
      RowBox[{"2", "m"}]], 
     RowBox[{"\[CapitalPsi]", "[", "x", "]"}]}]}], "\[Equal]", 
   RowBox[{"En", " ", 
    RowBox[{"\[CapitalPsi]", "[", "x", "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.739678055386846*^9, 3.739678171809202*^9}, {
