d197a97f6775"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   FractionBox[
    RowBox[{
     SuperscriptBox["A", "2"], " ", 
     RowBox[{"\[CapitalPsi]", "[", "x", "]"}]}], 
    RowBox[{"2", " ", "m"}]], "-", 
   FractionBox[
    RowBox[{"\[ImaginaryI]", " ", "A", " ", 
     RowBox[{
      SuperscriptBox["\[CapitalPsi]", "\[Prime]",
       MultilineFunction->None], "[", "x", "]"}]}], "m"], "-", 
   FractionBox[
    RowBox[{
     SuperscriptBox["\[CapitalPsi]", "\[Prime]\[Prime]",
