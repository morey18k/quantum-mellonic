d2aa81fc2ff3"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"eqn", "=", 
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"-", "1"}], 
      RowBox[{"2", " ", "m", " "}]], 
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"\[CapitalPsi]", "[", "x", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "2"}], "}"}]}], "]"}]}], "-", 
    RowBox[{
     FractionBox[
      RowBox[{"A", " ", "I"}], "m"], 
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"\[CapitalPsi]", "[", "x", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "1"}], "}"}]}], "]"}]}], "+", 
    RowBox[{
     FractionBox[
      SuperscriptBox["A", "2"], 
      RowBox[{"2", "m"}]], 
     RowBox[{"\[CapitalPsi]", "[", "x", "]"}]}]}], "\[Equal]", 
   RowBox[{"En", " ", 
    RowBox[{"\[CapitalPsi]", "[", "x", "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.739678055386846*^9, 3.739678171809202*^9}, {
  3.73967826016208*^9, 3.7396782644823523`*^9}, {3.7396784020515623`*^9, 
  3.739678406871696*^9}, {3.739678443789509*^9, 3.739678446910446*^9}, {
  3.739678554750249*^9, 3.7396785659569473`*^9}, {3.7396790009338512`*^9, 
  3.739679018822818*^9}, {3.739680210199736*^9, 3.7396802113912773`*^9}, {
  3.739701953715173*^9, 
  3.739701953722802*^9}},ExpressionUUID->"76313e0b-5e57-43b0-8e6c-\
e5151b23ae4b"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   FractionBox[
    RowBox[{
     SuperscriptBox["A", "2"], " ", 
     RowBox[{"\[CapitalPsi]", "[", "x", "]"}]}], 
    RowBox[{"2", " ", "m"}]], "-", 
   FractionBox[
    RowBox[{"\[ImaginaryI]", " ", "A", " ", 
     RowBox[{
      SuperscriptBox["\[CapitalPsi]", "\[Prime]",
       MultilineFunction->None], "[", "x", "]"}]}], "m"], "-", 
   FractionBox[
    RowBox[{
     SuperscriptBox["\[CapitalPsi]", "\[Prime]\[Prime]",
      MultilineFunction->None], "[", "x", "]"}], 
    RowBox[{"2", " ", "m"}]]}], "\[Equal]", 
  RowBox[{"En", " ", 
   RowBox[{"\[CapitalPsi]", "[", "x", "]"}]}]}]], "Output",
 CellChangeTimes->{3.739701959588328*^9, 3.7397639331157637`*^9, 
